{
  "name": "pokemongoapp",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.mjs",
  "scripts": {
    "start": "npm run migrate && node -r dotenv/config src/index.mjs",
    "migrate": "node -r esm -r dotenv/config ./node_modules/.bin/sequelize-cli db:migrate",
    "migrate:undo": "node -r esm -r dotenv/config ./node_modules/.bin/sequelize-cli db:migrate:undo",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "type=module c8 -c ./unit.c8rc mocha 'tests/unit/**/*.spec.*' dotenv_config_path=tests/.env.test",
    "lint": "eslint -c .eslintrc.yml --ext .mjs,.js .",
    "prepare": "husky install",
    "importCsv": "node ./src/readFiles/index.mjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "eslint": "^8.52.0",
    "esm": "^3.2.25",
    "exceljs": "^4.3.0",
    "express": "^4.18.2",
    "express-async-router": "^0.1.15",
    "express-http-proxy": "^2.0.0",
    "express-joi-validation": "^5.0.1",
    "express-request-id": "^3.0.0",
    "helmet": "^6.2.0",
    "husky": "^8.0.3",
    "joi": "^17.11.0",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.33.0"
  },
  "devDependencies": {
    "c8": "^8.0.1",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.4.0",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "nyc": "^15.1.0",
    "pre-commit": "^1.2.2",
    "sequelize-cli": "^6.6.1",
    "sinon": "^17.0.0"
  }
}
